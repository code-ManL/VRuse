{
  "name": "@vruse/vue",
  "version": "1.1.1",
  "description": "Collection of essential Vue Composition Utilities",
  "author": "LiuSeen<https://github.com/code-ManL>",
  "license": "MIT",
  "funding": "https://github.com/VR-use",
  "homepage": "https://github.com/VR-use/VRus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VR-use/VRuse",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/VR-use/VRuse/issues"
  },
  "keywords": ["vue", "react", "vr-use", "utils"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.mjs"
    },
    "./*": "./*",
    "./metadata": {
      "types": "./metadata.d.ts",
      "require": "./metadata.cjs",
      "import": "./metadata.mjs"
    }
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "unpkg": "./index.iife.min.js",
  "jsdelivr": "./index.iife.min.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@vruse/shared": "workspace:*",
    "vue-demi": "*"
  }
}
